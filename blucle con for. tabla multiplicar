# bucle tabla de multiplicar
def tabla():
    ntabla=int(input("Tabla del: "))
    for cont in range(1,11):
        print(str(int(ntabla*cont))

tabla()


# bucle tabla de multicar plus
def tabla_plus():
    ntabla=int(input("Tabla del: "))
    for cont in range(1,11):
        print(str(ntabla)+"*"+str(cont)+"= "+str(int(ntabla)*cont))

tabla_plus()
